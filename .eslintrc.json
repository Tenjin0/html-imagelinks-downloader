{
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "tabWidth": 4,
                "useTabs": true,
                "bracketSpacing": true
            }
        ],
        "no-console":"off"
    }
}